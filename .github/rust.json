{
    "problemMatcher": [
        {
            "owner": "rust-common",
            "pattern": [
                {
                    "regexp": "^(warning|warn|error)(\\[(.*)\\])?: (.*)$",
                    "severity": 1,
                    "message": 4,
                    "code": 3
                },
                {
                    "regexp": "^([\\s->=]*(.*):(\\d*):(\\d*)|.*)$",
                    "file": 2,
                    "line": 3,
                    "column": 4
                }
            ]
        },
        {
            "owner": "cargo-fmt",
            "pattern": [
                {
                    "regexp": "^(Diff in (\\S+)) at line (\\d+):",
                    "message": 1,
                    "file": 2,
                    "line": 3
                }
            ]
        }
    ]
}